{
    "preset": "laravel",
    "rules": {
        "simplified_null_return": false,
        "not_operator_with_successor_space": true,
        "ordered_imports": {
            "sort_algorithm": "alpha"
        },
        "no_unused_imports": true,
        "no_useless_else": true,
        "php_unit_method_casing": {
            "case": "camel_case"
        }
    },
    "exclude": [
        "vendor",
        "storage",
        "bootstrap/cache"
    ]
}
