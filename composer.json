{
    "name": "iamfarhad/laravel-rabbitmq",
    "description": "A robust RabbitMQ driver for Laravel Queue with advanced message queuing, reliable delivery, and high-performance async processing capabilities",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/iamfarhad/laravel-rabbitmq",
    "keywords": [
        "laravel",
        "laravel-queue",
        "rabbitmq",
        "amqp",
        "message-queue",
        "queue-driver",
        "async-processing",
        "message-broker",
        "distributed-systems",
        "microservices",
        "event-driven",
        "pub-sub",
        "reliable-messaging",
        "high-performance",
        "scalable",
        "php",
        "laravel-package",
        "queue-management",
        "background-jobs",
        "task-queue"
    ],
    "authors": [
        {
            "name": "Farhad Zand",
            "email": "farhad.pd@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/iamfarhad/laravel-rabbitmq/issues",
        "source": "https://github.com/iamfarhad/laravel-rabbitmq"
    },
    "require": {
        "php": "^8.2",
        "ext-amqp": "*",
        "ext-pcntl": "*",
        "illuminate/queue": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "ext-json": "*",
        "dg/bypass-finals": "dev-master",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^9.0|^10.0|^11.0",
        "phpstan/phpstan": "^1.10 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "iamfarhad\\LaravelRabbitMQ\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iamfarhad\\LaravelRabbitMQ\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:unit": "vendor/bin/phpunit --testsuite Unit",
        "test:feature": "vendor/bin/phpunit --testsuite Feature",
        "test:coverage": "vendor/bin/phpunit --coverage-html build/coverage",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "format-test": "vendor/bin/pint --test",
        "check": [
            "@format-test",
            "@analyse",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "iamfarhad\\LaravelRabbitMQ\\LaravelRabbitQueueServiceProvider"
            ],
            "aliases": {
                "RabbitMQ": "iamfarhad\\LaravelRabbitMQ\\Facades\\RabbitMQ"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}