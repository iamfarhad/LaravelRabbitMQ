{
    "name": "iamfarhad/laravel-rabbitmq",
    "description": "RabbitMQ driver for Laravel Queue with advanced message queuing capabilities",
    "keywords": [
        "laravel",
        "queue",
        "rabbitmq",
        "amqp",
        "message-queue",
        "php"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Farhad zand",
            "email": "farhad.pd@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-amqp": "*",
        "ext-pcntl": "*",
        "illuminate/queue": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "ext-json": "*",
        "dg/bypass-finals": "dev-master",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "iamfarhad\\LaravelRabbitMQ\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iamfarhad\\LaravelRabbitMQ\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage",
        "test-parallel": "vendor/bin/pest --parallel",
        "format": "vendor/bin/pint",
        "format-test": "vendor/bin/pint --test"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "iamfarhad\\LaravelRabbitMQ\\LaravelRabbitQueueServiceProvider"
            ],
            "aliases": {
                "RabbitMQ": "iamfarhad\\LaravelRabbitMQ\\Facades\\RabbitMQ"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}